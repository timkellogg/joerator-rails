<p id="notice"><%= notice %></p>

<h1>Listing Reviews</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Body</th>
      <th>Title</th>
      <th>Qualityrating</th>
      <th>Hipsterrating</th>
      <th>Studyrating</th>
      <th>Laptoprating</th>
      <th>Coffeeshop</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reviews.each do |review| %>
      <tr>
        <td><%= review.user %></td>
        <td><%= review.body %></td>
        <td><%= review.title %></td>
        <td><%= review.qualityRating %></td>
        <td><%= review.hipsterRating %></td>
        <td><%= review.studyRating %></td>
        <td><%= review.laptopRating %></td>
        <td><%= review.coffeeshop %></td>
        <td><%= link_to 'Show', review %></td>
        <td><%= link_to 'Edit', edit_review_path(review) %></td>
        <td><%= link_to 'Destroy', review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Review', new_review_path %>
